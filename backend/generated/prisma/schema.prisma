generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Add your models here
model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  name      String
  age       Int
  gender    String
  bio       String
  location  String
  pfpurl    String
  interests String[]
  createdAt DateTime @default(now())
}
